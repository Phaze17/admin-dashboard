# Environment Configuration for Phaze17 Admin Dashboard

# API Configuration
VITE_API_URL=https://api.phaze17.com
VITE_API_VERSION=v1

# Application Configuration
VITE_APP_TITLE=Phaze17 Admin Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional admin dashboard for Phaze17

# Authentication
VITE_AUTH_ENABLED=true
VITE_AUTH_PROVIDER=oauth2
VITE_AUTH_REDIRECT_URI=http://localhost:5173/auth/callback

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_NOTIFICATIONS=true

# Development
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS=false